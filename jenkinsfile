pipeline

   agent any
   tools {
        maven "MVN"
        jdk "JDK-11"
    }

   stages {
      stage('Verify Branch') {
         steps {
            echo "$GIT_BRANCH"
            echo "ahmedfathyy"
         }
      }
      stage('Build with maven') {
        steps {
            sh 'mvn clean package'
            }
        }
    }