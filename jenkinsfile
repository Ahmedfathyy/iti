pipeline {

   agent any
   tools {
        maven "MVN"
        jdk "JDK-11"
    }

   stages {
      stage('Verify Branch') {
         steps {
            echo "$GIT_BRANCH"
            echo  "$JAVA_HOME"
            
         }
      }
      stage('Build with maven') {
        steps {
            sh 'mvn clean package'
            
            }
        }
      stage('run unit testing') { 
       steps {
         sh 'mvn test'
         }
      } 
    }
}